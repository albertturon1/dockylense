{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "test": {},
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "generate:types": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "compile:macos": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
