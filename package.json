{
  "name": "dockylense",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "generate:types": "turbo generate:types",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "lint:fix": "pnpm turbo lint -- --fix"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "turbo": "^2.5.8"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.0.0"
  }
}
